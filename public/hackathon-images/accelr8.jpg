<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#0a0a0a" />
  
  <!-- Neon glow effect -->
  <filter id="neon" x="-50%" y="-50%" width="200%" height="200%">
    <feGaussianBlur stdDeviation="10" result="blur" />
    <feComposite in="SourceGraphic" in2="blur" operator="over" />
  </filter>
  
  <!-- Background grid -->
  <g stroke="#1a1a1a" stroke-width="1">
    <g id="grid">
      <line x1="0" y1="0" x2="800" y2="0" />
      <line x1="0" y1="50" x2="800" y2="50" />
      <line x1="0" y1="100" x2="800" y2="100" />
      <line x1="0" y1="150" x2="800" y2="150" />
      <line x1="0" y1="200" x2="800" y2="200" />
      <line x1="0" y1="250" x2="800" y2="250" />
      <line x1="0" y1="300" x2="800" y2="300" />
      <line x1="0" y1="350" x2="800" y2="350" />
      <line x1="0" y1="400" x2="800" y2="400" />
      <line x1="0" y1="450" x2="800" y2="450" />
      <line x1="0" y1="500" x2="800" y2="500" />
      <line x1="0" y1="550" x2="800" y2="550" />
      <line x1="0" y1="600" x2="800" y2="600" />
      
      <line x1="0" y1="0" x2="0" y2="600" />
      <line x1="50" y1="0" x2="50" y2="600" />
      <line x1="100" y1="0" x2="100" y2="600" />
      <line x1="150" y1="0" x2="150" y2="600" />
      <line x1="200" y1="0" x2="200" y2="600" />
      <line x1="250" y1="0" x2="250" y2="600" />
      <line x1="300" y1="0" x2="300" y2="600" />
      <line x1="350" y1="0" x2="350" y2="600" />
      <line x1="400" y1="0" x2="400" y2="600" />
      <line x1="450" y1="0" x2="450" y2="600" />
      <line x1="500" y1="0" x2="500" y2="600" />
      <line x1="550" y1="0" x2="550" y2="600" />
      <line x1="600" y1="0" x2="600" y2="600" />
      <line x1="650" y1="0" x2="650" y2="600" />
      <line x1="700" y1="0" x2="700" y2="600" />
      <line x1="750" y1="0" x2="750" y2="600" />
      <line x1="800" y1="0" x2="800" y2="600" />
    </g>
  </g>
  
  <!-- Main Logo -->
  <g transform="translate(400, 250)">
    <text x="0" y="-40" font-family="monospace" font-size="80" font-weight="bold" fill="#4ade80" text-anchor="middle" filter="url(#neon)">1412</text>
    <text x="0" y="40" font-family="monospace" font-size="60" font-weight="bold" fill="#4ade80" text-anchor="middle" filter="url(#neon)">HACKER</text>
    <text x="0" y="100" font-family="monospace" font-size="40" font-weight="bold" fill="#4ade80" text-anchor="middle" filter="url(#neon)">HOTEL</text>
  </g>
  
  <!-- OPEN HOUSE text -->
  <text x="400" y="400" font-family="Arial" font-size="30" font-weight="bold" fill="white" text-anchor="middle">OPEN HOUSE</text>
  
  <!-- Date and Location -->
  <text x="400" y="450" font-family="Arial" font-size="20" fill="#94a3b8" text-anchor="middle">June 21, 2023 • 1:00 PM • 1412 Market St</text>
  
  <!-- Decorative elements -->
  <rect x="150" y="500" width="500" height="5" fill="#4ade80" filter="url(#neon)" />
</svg>